# Supabase
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Stripe
STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key
STRIPE_SECRET_KEY=sk_test_your_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# Google AI (Gemini/Veoa)
GOOGLE_AI_API_KEY=your_google_ai_api_key
GOOGLE_AI_PROJECT_ID=your_google_ai_project_id
GOOGLE_AI_LOCATION=us-central1

# Database (for local development)
DATABASE_URL=postgresql://postgres:postgres@localhost:54322/postgres

# FFmpeg (if needed for video processing)
FFMPEG_PATH=/usr/bin/ffmpeg
FFPROBE_PATH=/usr/bin/ffprobe

# YoUtube OAuth (placeholders)
YOUTUBE_OAUTH_CLIENT_ID=your_youtube_client_id
YOUTUBE_OAUTH_CLIENT_SECRET=your_youtube_client_secret

# TikTok (placeholders)
TIKTOK_CLIENT_ID=your_tiktok_client_id
TIKTOK_CLIENT_SECRET=your_tiktok_client_secret

# Meta (placeholders)
META_CLIENT_ID=your_meta_client_id
META_CLIENT_SECRET=your_meta_client_secret

# Gemini API Key (renamed from Google AI)
GEMINI_API_KEY=your_gemini_api_key

# VEO Model (optional, defaults to veo-3)
VEO_MODEL=veo-3

# Next.js Public Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
